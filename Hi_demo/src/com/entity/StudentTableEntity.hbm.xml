<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.entity.StudentTableEntity" table="student_table" schema="user">
        <id name="studentId" column="student_id"/>
        <property name="studnetName" column="studnet_name"/>
        <property name="studentSex" column="student_sex"/>
        <property name="studentBirth" column="student_birth"/>
        <property name="classId" column="class_id"/>
    </class>
</hibernate-mapping>